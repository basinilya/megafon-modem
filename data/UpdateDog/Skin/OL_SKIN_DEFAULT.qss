 /*Add by zhoujun 2010-12-8 begin*/
 QComboBox{
    border-image: url(./Skin/res/edit_ctrl_border_line_box.png) 1 2 1 2;
    border-width: 1px;
    border-style: solid;
}  
QComboBox:!enabled {
     background-color: rgb(231, 231, 231);    
}


QComboBox::drop-down  { 	
	image: url(./Skin/res/combobox_down_arrow_normal.png);	
}

QComboBox::drop-down:hover	 	{ image: url(./Skin/res/combobox_down_arrow_hover.png);}
QComboBox::drop-down:!enabled	{ image: url(./Skin/res/combobox_down_arrow_gray.png);}
/*Add by zhoujun 2010-12-8 end */
  
 QLineEdit[echoMode="2"] {
     lineedit-password-character: 9679;
 }
  
 QLineEdit:read-only {
	background: white;
	background-color: white;
}

 QLineEdit:editable {    
     border-image: url(./Skin/res/tree_widget_bg_line_box.png)1 1 1 1;
     border-top-width: 1px;
     border-right-width: 1px;
     border-bottom-width: 1px;
     border-left-width: 1px; 
     background: white;
     selection-color: white;
     selection-background-color: rgb(10, 36, 106);
 }

 QProgressBar {
     border-image: url(./Skin/res/progressbar_unbg.png) 2 2 2 2;
    border-top-width: 1px;
	border-right-width: 1px;
	border-bottom-width: 1px;
	border-left-width: 1px;	
	height: 15px;
	text-align: center;
 }

 QProgressBar::chunk {
     border-image: url(./Skin/res/progressbar_update_bg.png);
 }



QLineEdit{
    border-image: url(./Skin/res/widget_bg_line_box.png);
    border-width: 1px;
    border-style: solid;
}
/*=============QRadioButton============*/

QRadioButton::indicator:!enabled:unchecked  { image: url(./Skin/res/radio_button_disabled.png);       }
QRadioButton::indicator:!enabled:checked    { image: url(./Skin/res/sel_radio_button_disabled.png);   }
QRadioButton::indicator:unchecked           { image: url(./Skin/res/radio_button_normal.png);           }
QRadioButton::indicator:unchecked:hover     { image: url(./Skin/res/radio_button_hover.png);       }
QRadioButton::indicator:unchecked:pressed   { image: url(./Skin/res/radio_button_hover.png);       }
QRadioButton::indicator::checked            { image: url(./Skin/res/sel_radio_button_normal.png);       }
QRadioButton::indicator:checked:hover 	    { image: url(./Skin/res/sel_radio_button_hover.png);   }
QRadioButton::indicator:checked:pressed     { image: url(./Skin/res/sel_radio_button_hover.png);   }
 
 /*=============QCheckBox============*/

QCheckBox::indicator:!enabled:unchecked     { image: url(./Skin/res/unsel_check_box_disabled.png);     }
QCheckBox::indicator:!enabled:checked       { image: url(./Skin/res/checkbox_checked_disabled.png);           }
QCheckBox::indicator:unchecked              { image: url(./Skin/res/unsel_check_box_unchecked.png);     }
QCheckBox::indicator:unchecked:hover        { image: url(./Skin/res/unsel_check_box_hover.png);     }
QCheckBox::indicator:unchecked:selected     { image: url(./Skin/res/unsel_check_box_hover.png);     }
QCheckBox::indicator:checked                { image: url(./Skin/res/checkbox_checked_normal.png);           }
QCheckBox::indicator:checked:hover          { image: url(./Skin/res/checkbox_checked_hover.png);           }
QCheckBox::indicator:checked:pressed        { image: url(./Skin/res/checkbox_checked_pressed.png);           }
QCheckBox::indicator:indeterminate:hover    { image: url(./Skin/res/unsel_check_box_hover.png);     }
QCheckBox::indicator:indeterminate:selected { image: url(./Skin/res/checkbox_checked_hover.png);           }
