/* ========== QHeaderView ==========    */
 QHeaderView {
    border-image: url(./Skin/res/table_header_bg.png);
 }

 QHeaderView::section {
    border-image: url(./Skin/res/table_header_bg.png);
    padding-left: 2px;
 }


 QHeaderView::section:hover {
    border-image: url(./Skin/res/table_header_hover.png);
 }

 QHeaderView::section:pressed {
    border-image: url(./Skin/res/table_header_press.png);
 }

 QHeaderView::section::margin
 {
  image: url(./Skin/res/table_header_separator.png);
  image-position: right;
 }

 /*=============QScrollBar Horizontal ============*/
  QScrollBar::horizontal {
     background-image: url("./Skin/res/messagebox_bg.png");
     height: 18px;
     margin: 0px 18px 0px 18px;
 }
 
 
 QScrollBar::handle:horizontal {
     border-image: url(./Skin/res/scrollbar_horizontal_normal.png) 2 2 2 2;
     border-top-width: 2px;
     border-right-width: 2px;
     border-bottom-width: 2px;
     border-left-width: 2px;
     min-width: 20px;
 }
 
 QScrollBar::handle:horizontal:hover {
     border-image: url(./Skin/res/scrollbar_horizontal_hover.png) 2 2 2 2;
     border-top-width: 2px;
     border-right-width: 2px;
     border-bottom-width: 2px;
     border-left-width: 2px;
     min-width: 20px;
 }
 
 QScrollBar::handle:horizontal:pressed {
     border-image: url(./Skin/res/scrollbar_horizontal_press.png) 2 2 2 2;
     border-top-width: 1px;
     border-right-width: 1px;
     border-bottom-width: 1px;
     border-left-width: 1px;
     min-width: 20px;
 }
  

 QScrollBar::add-line:horizontal {
     background: rgb(92, 162, 213);
     width: 18px;
     subcontrol-position: right;
     subcontrol-origin: margin;
 }

 QScrollBar::sub-line:horizontal {
     background: rgb(92, 162, 213);
     width: 18px;     
     subcontrol-position: left;
     subcontrol-origin: margin;
 }

  QScrollBar:left-arrow:horizontal { border-image: url(./Skin/res/popup_controls_button_left.png);  }
 QScrollBar:left-arrow:horizontal:hover { border-image: url(./Skin/res/popup_controls_button_left_hover.png);   }
  QScrollBar:right-arrow:horizontal { border-image: url(./Skin/res/popup_controls_button_right.png);   }
 QScrollBar:right-arrow:horizontal:hover { border-image: url(./Skin/res/popup_controls_button_right_hover.png);   }
 
 /*=============QScrollBar Vertical ============*/
QScrollBar::vertical {
     background: rgb(221, 241, 255);
     width: 18px;
     margin: 18px 0px 18px 0px;
 }
 
 QScrollBar::handle:vertical {
     border-image: url(./Skin/res/scrollbar_vertical_normal.png) 2 2 2 2;
     border-top-width: 2px;
     border-right-width: 2px;
     border-bottom-width: 2px;
     border-left-width: 2px;
     min-height: 20px;
 }
 QScrollBar::handle:vertical:hover {
     border-image: url(./Skin/res/scrollbar_vertical_hover.png) 2 2 2 2;
     border-top-width: 2px;
     border-right-width: 2px;
     border-bottom-width: 2px;
     border-left-width: 2px;
     min-height: 20px;
 }
 
 QScrollBar::handle:vertical:pressed {
     border-image: url(./Skin/res/scrollbar_vertical_press.png) 2 2 2 2;
     border-top-width: 2px;
     border-right-width: 2px;
     border-bottom-width: 2px;
     border-left-width: 2px;
     min-height: 20px;
 }
  
 QScrollBar::add-line:vertical {
     background: rgb(92, 162, 213);
     height: 18px;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
 }

 QScrollBar::sub-line:vertical {
     background: rgb(92, 162, 213);
     height: 18px;
     subcontrol-position: top;
     subcontrol-origin: margin;
 }

 
QScrollBar:up-arrow:vertical 			{ border-image: url(./Skin/res/popup_controls_button_up.png);   }
QScrollBar:up-arrow:vertical:hover 		{ border-image: url(./Skin/res/popup_controls_button_up_hover.png);   }
QScrollBar:down-arrow:vertical 			{ border-image: url(./Skin/res/popup_controls_button_down.png);   }
QScrollBar:down-arrow:vertical:hover 	{ border-image: url(./Skin/res/popup_controls_button_down_hover.png);   }

